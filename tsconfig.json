{
  "include": [
    "src/**/*",
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "build",
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
    },
    "noEmit": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error"
  }
}